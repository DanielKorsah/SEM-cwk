<Window x:Class="SEM_cwk.Rate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SEM_cwk"
        mc:Ignorable="d"
        Title="Rate" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="rating" HorizontalAlignment="Left" Height="23" Margin="77,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="53"/>
        <Button Content="Confirm" HorizontalAlignment="Left" Margin="77,83,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
